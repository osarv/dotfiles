filetype plugin indent on
syntax on
colo torte
set mouse=a
set tabstop=4
set shiftwidth=4
set softtabstop=4
set autoindent
set smartindent
set number
set showmatch
set ignorecase
set incsearch
set nohlsearch
set expandtab
set nobackup
set noswapfile
set cursorline
set cursorlineopt=number
hi clear CursorLineNr
hi CursorLineNr ctermfg=magenta
set complete+=i
